
<table class="table">
	<thread>
		<tr>
			<th>Имя</th>
			<th>Телефон</th>
			<th>Даты</th>
			<th>Парикмахер</th>
			<th>Цвет покраски</th>
		</tr>
	</thread>
<% @results.each do |row| %>
	<body>
		<tr>
			<td><%= row['username'] %></td>
			<td><%= row['phone'] %></td>
			<td><%= row['datestamp'] %></td>
			<td><%= row['barber'] %></td>
			<td style="background-color: <%= row['color'] %>"><%= row['color'] %></td>
		</tr>
	</body>
<% end %>

</table>

